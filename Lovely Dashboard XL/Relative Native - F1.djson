        {
          "$type": "SimHub.Plugins.OutputPlugins.GraphicalDash.Models.Layer, SimHub.Plugins",
          "BackgroundColor": "#00FFFFFF",
          "Bindings": {
            "Visible": {
              "Formula": {
                "Expression": "if(isnull(drivercarname([DataCorePlugin.GameData.Position] + 10)), 0, [DataCorePlugin.GameData.Position]<2)"
