              "Bindings": {
                "Text": {
                  "Formula": {
                    "Expression": "const json_settings = readtextfile('./JavascriptExtensions/Lovely-Dashboard_settings.json')\r\nconst settings = JSON.parse(json_settings);\r\n\r\nif ( !settings ) {\r\n\tdriverName = 0\r\n} else {\r\n\tdriverName = settings.driverName\r\n}\r\n\r\nvar position = $prop('DataCorePlugin.GameData.Position') + 10\r\n\r\nvar name = drivername(position).replace('\ufffd0', 'E');\r\nvar full_name = name.split(' ');\r\n\r\nif (full_name.len > 1) {\r\n\tif (driverName == 1) {\r\n\t\treturn full_name[0] + ' ' + full_name[full_name.length-1].substr(0,1) + '.'\r\n\t} else if (driverName == 2) {\r\n\t\treturn full_name[0] + ' ' + full_name[full_name.length-1]\r\n\t}else {\r\n\t\treturn full_name[0].substr(0,1) + '. ' + full_name[full_name.length-1]\r\n\t}\r\n} else {\r\n\treturn name\r\n}",
