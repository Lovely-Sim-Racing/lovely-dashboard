          "Childrens": [
            {
              "$type": "SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins",
              "BackgroundColor": "#00FFFFFF",
              "Bindings": {
                "Text": {
                  "FormatString": "",
                  "Formula": {
                    "Expression": "function isRace(sessionType) {\r\n\treturn sessionType == 10 || sessionType == 11 || sessionType == 12\r\n}\r\n\r\nposition = $prop('DataCorePlugin.GameData.Position') + 10\r\n\r\nif (isRace($prop('GameRawData.PacketSessionData.m_sessionType'))) {\r\n\tif ($prop('GarySwallowDataPlugin.Leaderboard.Position' + (position < 10 ? '0' + position : position) + '.Finished')) {\r\n\t\treturn '-'\r\n\t}\r\n\telse if (Math.abs(drivergaptoleader(position)) > 0) {\r\n\t\treturn format(Math.abs(drivergaptoleader(position)), '+0.0')\r\n\t} else {\r\n\t\treturn '0.0'\r\n\t}\r\n} else {\r\n\tvar PosBestLap = timespantoseconds(driverbestlap(position));\r\n\tvar PlayerBestLap = timespantoseconds($prop('BestLapTime'))\r\n\tvar PracticeQualifyGap = 0;\r\n\t\t\r\n\tif (PlayerBestLap > 0 && PosBestLap > 0) {\r\n\t\tPracticeQualifyGap = PosBestLap - PlayerBestLap\r\n\t\tif (PracticeQualifyGap > 0) {\r\n\t\t\tPracticeQualifyGap = format(PracticeQualifyGap, '+0.0')\r\n\t\t} else if (PracticeQualifyGap < 0) {\r\n\t\t\tPracticeQualifyGap = format(PracticeQualifyGap, '0.0')\r\n\t\t} else {\r\n\t\t\tPracticeQualifyGap = '0.0'\r\n\t\t}\r\n\t} else {\r\n\t\tPracticeQualifyGap = '-'\r\n\t}\r\n\t\r\n\treturn PracticeQualifyGap\r\n}",
                    "Interpreter": 1
                  },
                  "Mode": 2,
                  "TargetPropertyName": "Text"
                },
                "TextColor": {
                  "Formula": {
                    "Expression": "function isRace(sessionType) {\r\n\treturn sessionType == 10 || sessionType == 11 || sessionType == 12\r\n}\r\n\r\nvar position = $prop('DataCorePlugin.GameData.Position') + 10\r\n\r\nvar driver_gap = drivergaptoplayer(position);\r\n\r\nvar driver_lap = timespantoseconds(isnull(driverbestlap($prop('DataCorePlugin.GameData.BestLapOpponentPosition')+1), '0:00.000'))\r\n\r\n\r\nif (driver_lap > 0 && isRace($prop('GameRawData.PacketSessionData.m_sessionType'))) {\r\n\r\n\tif (drivercurrentlap(position) - $prop('DataCorePlugin.GameData.CurrentLap') >= 2 ) {\r\n\t\t\r\n\t\treturn '#FF7400' // 2+ Laps Ahead\r\n\t\t\r\n\t} else if (drivercurrentlap(position) - $prop('DataCorePlugin.GameData.CurrentLap') <= -2 ) {\r\n\t\t\r\n\t\treturn '#00BFFF' // 2- Laps Behind\r\n\t\t\r\n\t} else {\r\n\t\r\n\t\tif ( driver_lap + driver_gap < (driver_lap * 0.15) ) {\r\n\t\t\t\r\n\t\t\treturn '#FF7400' // Ahead\r\n\t\t\t\r\n\t\t} else if ( driver_lap + driver_gap > driver_lap+(driver_lap * 0.85) ) {\r\n\t\t\r\n\t\t\treturn '#00BFFF' // Behind\r\n\t\t\t\r\n\t\t} else {\r\n\t\t\r\n\t\t\treturn '#FFFFFF' // Same Lap\r\n\t\t\t\r\n\t\t}\r\n\r\n\t}\r\n\t\r\n} else {\r\n\r\n\treturn '#FFFFFF'\r\n\r\n}",
                    "Interpreter": 1
                  },
                  "Mode": 2,
                  "TargetPropertyName": "TextColor"
                },
                "Top": {
                  "Formula": {
                    "Expression": "if([DataCorePlugin.GameData.Position]>[OpponentsCount]-5, ([DataCorePlugin.GameData.Position]-([OpponentsCount]-20)) * 29,\r\nif([DataCorePlugin.GameData.Position]>6, (6 + 9) * 29,\r\n([DataCorePlugin.GameData.Position] + 9) * 29))"
                  },
                  "Mode": 2,
                  "TargetPropertyName": "Top"
