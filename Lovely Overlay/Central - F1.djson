        {
          "$type": "SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins",
          "BackgroundColor": "#FF0064CF",
          "Bindings": {
            "BackgroundColor": {
              "Formula": {
                "Expression": "var gray = \"#FF262626\"\r\nvar red = \"#FFB20000\"\r\nvar blue = \"#FF0064CF\"\r\nvar purple = \"#FFA100A9\"\r\nvar green = \"#FF009F01\"\r\n\r\nif ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==0) {\r\n    return green\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==1) {\r\n    return blue\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==2) {\r\n    return red\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==3) {\r\n    return purple\r\n} else {\r\n    return gray\r\n}",
                "Interpreter": 1
              },
              "Mode": 2,
              "TargetPropertyName": "BackgroundColor"
            },
            "Text": {
              "Formula": {
                "Expression": "if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==0) {\r\n    return \"DISABLED\"\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==1) {\r\n    return \"MEDIUM\"\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==2) {\r\n    return \"HOTLAP\"\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==3) {\r\n    return \"OVERTAKE\"\r\n} else {\r\n    return \"ERS\" + $prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode')\r\n}",
                "Interpreter": 1
              },
              "Mode": 2,
              "TargetPropertyName": "Text"
            },
            "TextColor": {
              "Formula": {
                "Expression": "if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==0) {\r\n    return \"Black\"\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==1) {\r\n    return \"White\"\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==2) {\r\n    return \"White\"\r\n} else if ($prop('DataCorePlugin.GameRawData.PlayerCarStatusData.m_ersDeployMode') ==3) {\r\n    return \"White\"\r\n} else {\r\n    return \"White\"\r\n}",
                "Interpreter": 1
              },
              "Mode": 2,
