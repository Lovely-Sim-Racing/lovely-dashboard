        {
          "$type": "SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins",
          "BackgroundColor": "#FF878787",
          "Bindings": {
            "BackgroundColor": {
              "Formula": {
                "Expression": "gray =   '#FF878787'\r\nred =    '#FFFA0000'\r\nyellow = '#FFFFE04C'\r\ngreen =  '#FF00FF02'\r\npurple = '#FFC500CE'\r\n\r\nfastestLap = timespantoseconds(isnull(driverbestlap($prop('DataCorePlugin.GameData.BestLapOpponentPosition')+1), '0:00.000'))\r\n\r\nif ( ! timespantoseconds($prop('DataCorePlugin.GameData.BestLapTime')) > 0) {\r\n\treturn gray\r\n} else if ( $prop('GameRawData.PlayerLapData.m_currentLapInvalid') == 0) {\r\n\tvar timeDiffMine = timespantoseconds($prop('PersistantTrackerPlugin.EstimatedLapTime')) - timespantoseconds( $prop('DataCorePlugin.GameData.BestLapTime') )\r\n\t\r\n\tvar timeDiffOverall = timespantoseconds($prop('PersistantTrackerPlugin.EstimatedLapTime')) - fastestLap\r\n\t\r\n\tif ( timeDiffMine > 0 ) {\r\n\t\r\n\t\treturn yellow\r\n\t\t\r\n\t} else {\r\n\t\r\n\t\tif ( timeDiffOverall > 0 ) {\r\n\t\r\n\t\t\treturn green\r\n\t\t\r\n\t\t} else {\r\n\t\r\n\t\t\treturn purple\r\n\t\t\r\n\t\t}\r\n\t\t\r\n\t}\r\n\r\n} else {\r\n\treturn red\r\n}",
                "Interpreter": 1
              },
              "Mode": 2,
              "TargetPropertyName": "BackgroundColor"
            },
            "Text": {
              "FormatString": "0.000",
              "Formula": {
                "Expression": "var timeDiffMine = timespantoseconds($prop('DataCorePlugin.GameData.BestLapTime')) > 0 ?\r\n\ttimespantoseconds($prop('PersistantTrackerPlugin.EstimatedLapTime')) - timespantoseconds( $prop('DataCorePlugin.GameData.BestLapTime') ) : 0\r\n\r\nreturn timeDiffMine >= 0 ? format(timeDiffMine,'+0.000') : format(timeDiffMine, '0.000')",
                "Interpreter": 1
              },
              "Mode": 2,
              "TargetPropertyName": "Text"
            },
            "TextColor": {
              "Formula": {
                "Expression": "black =  '#FF000000'\r\nwhite =  '#FFFFFFFF'\r\n\r\nfastestLap = timespantoseconds(isnull(driverbestlap($prop('DataCorePlugin.GameData.BestLapOpponentPosition')+1), '0:00.000'))\r\nif ( $prop('GameRawData.PlayerLapData.m_currentLapInvalid') == 0 &&\r\n     timespantoseconds($prop('DataCorePlugin.GameData.BestLapTime')) > 0) {\r\n\tvar timeDiffMine = timespantoseconds($prop('PersistantTrackerPlugin.EstimatedLapTime')) - timespantoseconds( $prop('DataCorePlugin.GameData.BestLapTime') )\r\n\t\r\n\tvar timeDiffOverall = timespantoseconds($prop('PersistantTrackerPlugin.EstimatedLapTime')) - fastestLap\r\n\t\r\n\tif ( timeDiffMine > 0 ) {\r\n\t\r\n\t\treturn black\r\n\t\t\r\n\t} else {\r\n\t\r\n\t\tif ( timeDiffOverall > 0 ) {\r\n\t\r\n\t\t\treturn black\r\n\t\t\r\n\t\t} else {\r\n\t\r\n\t\t\treturn white\r\n\t\t\r\n\t\t}\r\n\t\t\r\n\t}\r\n\r\n} else {\r\n\treturn white\r\n}\r\n",
                "Interpreter": 1
              },
              "Mode": 2,
