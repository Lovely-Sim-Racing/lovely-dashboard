        },
        {
          "$type": "SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins",
          "BackgroundColor": "#00FFFFFF",
          "Bindings": {
            "Text": {
              "FormatString": "mm\\:ss\\.fff",
              "Formula": {
                "Expression": "if ( timespantoseconds($prop('PersistantTrackerPlugin.EstimatedLapTime')) == 0 ) {\r\n\treturn $prop('CurrentLapTime')\r\n} else {\r\n\treturn $prop('PersistantTrackerPlugin.EstimatedLapTime')\r\n}",
                "Interpreter": 1
              },
              "Mode": 2,
              "TargetPropertyName": "Text"
            },
            "TextColor": {
              "Formula": {
